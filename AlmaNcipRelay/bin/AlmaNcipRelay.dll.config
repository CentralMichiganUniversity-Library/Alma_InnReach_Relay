    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <add key="AlmaNcipUrl" value="https://na01.alma.exlibrisgroup.com/view/NCIPServlet/v1" />
    <add key="InnReachSchemeTag" value="http://72.52.134.169:6601/IRCIRCD?target=get_scheme_values&amp;scheme=UniqueAgencyId" />
    <add key="AlmaInstitutionCode" value="{your alma institution code}" />
    <add key="AlmaInstitutionName" value="{your institution name according to InnReach}" />
    <add key="InnReachUserGroup" value="Adult" />
    <add key="InnReachSiteCode" value="{your InnReach site code}" />
    <add key="AlmaNcipProfileCode" value="{yoru Alma resource sharing partner code for InnReach" />
    <add key="UpgradeItemCheckOutRequest" value="true" />
    <add key="APICheckoutLibrary" value="{library to use for API checkouts}"/>
    <add key="APiCheckoutDesk" value="{desk to use for checkouts}"/>
    <add key="CheckoutApIUrl" value="https://api-na.hosted.exlibrisgroup.com/almaws/v1/users/{0}/loans?apiKey={your api key here}&amp;item_barcode={1}"/>
    <add key="ChangeDateApiUrl" value="https://api-na.hosted.exlibrisgroup.com/almaws/v1/users/{0}/loans/{1}?apiKey={your api key here}"/>
    <add key="GetLoansApiUrl" value="https://api-na.hosted.exlibrisgroup.com/almaws/v1/users/{0}/loans?apiKey={your api key here}"/>
  </appSettings>
  <system.web>
